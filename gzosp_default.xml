<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="gzosp"
           fetch="https://github.com/GZOSP"
           sync-c="true"
           sync-f="true"
           sync-j="8"
           revision="8.0" />

  <!-- GZOSP -->
  <project path="manifest" name="manifest" remote="gzosp" />
  <project path="build/make" name="build_make" remote="gzosp" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/blueprint" name="platform/build/blueprint" groups="pdk,tradefed" />
  <project path="build/kati" name="build_kati" groups="pdk,tradefed" remote="gzosp" />
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="gzosp" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <project path="frameworks/base" name="frameworks_base" remote="gzosp" />
  <project path="hardware/libhardware" name="hardware_libhardware" remote="gzosp" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="gzosp" />
  <project path="system/core" name="system_core" groups="pdk" remote="gzosp" />
  <project path="system/media" name="system_media" groups="pdk" remote="gzosp" />
  <project path="vendor/gzosp" name="vendor_gzosp" remote="gzosp" />

</manifest>
